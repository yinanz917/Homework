%Question 2
var count : int
get count

var list : array 1 .. count of string
var sorted : array 1 .. count of string
var where : int
var current_first : string
var last : string

for i : 1 .. count
    get list (i) 
end for

last := list (1)

for i : 2 .. count
    if list (i) > last then
        last := list (i)
    end if
end for

for i : 1 .. count 
    current_first := list (1)
    for j : 1 .. count 
        if list (j) <= current_first then
            current_first := list (j)
            where := j
        end if
    end for
    sorted (i) := current_first
    list (where) := chr (127)
end for

colour (red)
put "Here's the ", count," words in alphabetical order: " 
for i : 1 .. count
    colour (black)
    put sorted (i)
end for


%Extra Question
var count : int
get count

var list : array 1 .. count of int
var sorted : array 1 .. count of int
var amount_in_sorted : int := 0

function isNotDuplicate (number : int) : boolean
    for j : 1 .. amount_in_sorted
        if number = list (j) then
            result false
        end if
    end for
    result true
end isNotDuplicate

for i : 1 .. count
    get list (i)
end for

sorted (1) := list (1)
amount_in_sorted += 1

for i : 2 .. count
    if isNotDuplicate (list (i)) then
        amount_in_sorted += 1
        sorted (amount_in_sorted) := list (i)
    end if
end for

for i : 1 .. amount_in_sorted
    put sorted (i)
end for







